var tmp = [];
var isPalindrome = function(x) {
  tmp =  Array.from(String(x), Number);
  let leftIndex = -1;
  let rightIndex = tmp.length;
  if(tmp.length == 0){
    return false;
  }
   
  return checkPlindrome(leftIndex, rightIndex);
};

var checkPlindrome = function(leftIndex, rightIndex){
  let startIndex = leftIndex + 1;
  let lastIndex = rightIndex - 1;
  
  if(startIndex > lastIndex){
    return true;
  }
  if(tmp[startIndex] != tmp[lastIndex]){
     return false; 
  }
  
   return checkPlindrome(startIndex, lastIndex);
}



function isPalindromeS2(x){
  let reverseNum = 0;
  let tmp = x;
  while(tmp > 0){
    reverseNum = reverseNum*10 + tmp%10;
    tmp = Math.floor(tmp/10);
   }
  return x == reverseNum;
}
