/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    let tmp = []; 
    for(let i = 0, count = nums.length; i < count ;i++){
      for(let j = (i + 1); j < count;j++){
         if((nums[i] + nums[j]) == target){
          tmp.push(i)
          tmp.push(j)
         
          break;
        }
      }
     
    }
    return tmp;
};
